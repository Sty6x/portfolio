---
import pdfReference from "../assets/pdf/Jeryl Franz Diaz Benjamin.pdf";
import ResumeLayout from "../layouts/ResumeLayout.astro";
---

<ResumeLayout title="Jeryl Franz Benjamin Resume">
	<div class="container">
		<embed src={pdfReference} type="application/pdf" />
	</div>
</ResumeLayout>

<style>
	.container {
		display: flex;
		flex: 1;
		/* width: 100%; */
		height: inherit;
	}
	embed {
		width: 100%;
		height: 100%;
	}
</style>
