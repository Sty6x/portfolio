---
import MainLayout from "../layouts/MainLayout.astro";
import Layout from "../layouts/Layout.astro";
---

<Layout title="Jeryl Franz Benjamin Resume">
	<div class="container">
		<embed
			src="../../public/pdf/Jeryl Franz Benjamin Resume.pdf"
			type="application/pdf"
		/>
	</div>
</Layout>

<style>
	.container {
		display: flex;
		flex: 1;
		width: 100%;
		height: 100%;
	}
	embed {
		width: 100%;
		height: 100%;
	}
</style>
